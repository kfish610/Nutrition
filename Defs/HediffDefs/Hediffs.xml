<?xml version="1.0" encoding="utf-8"?>
<Defs xmlns="https://github.com/kfish610/RimWorldSchema" 
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://github.com/kfish610/RimWorldSchema https://github.com/kfish610/RimworldSchema/blob/master/schema.xsd?raw=true">
    <HediffDef>
        <defName>MissingVegetables</defName>
        <label>missing vegetables</label>
        <stages>
            <li>
                <label>trivial</label>
                <capMods>
                    <li>
                        <capacity>Sight</capacity>
                        <offset>-0.05</offset>
                    </li>
                    <li>
                        <capacity>Hearing</capacity>
                        <offset>-0.05</offset>
                    </li>
                </capMods>
            </li>
            <li>
                <minSeverity>0.5</minSeverity>
                <label>minor</label>
                <capMods>
                    <li>
                        <capacity>Sight</capacity>
                        <offset>-0.10</offset>
                    </li>
                    <li>
                        <capacity>Hearing</capacity>
                        <offset>-0.10</offset>
                    </li>
                </capMods>
            </li>
            <li>
                <minSeverity>1.0</minSeverity>
                <label>moderate</label>
                <capMods>
                    <li>
                        <capacity>Sight</capacity>
                        <offset>-0.20</offset>
                    </li>
                    <li>
                        <capacity>Hearing</capacity>
                        <offset>-0.20</offset>
                    </li>
                </capMods>
            </li>
            <li>
                <minSeverity>1.5</minSeverity>
                <label>severe</label>
                <capMods>
                    <li>
                        <capacity>Sight</capacity>
                        <offset>-0.30</offset>
                    </li>
                    <li>
                        <capacity>Hearing</capacity>
                        <offset>-0.30</offset>
                    </li>
                </capMods>
            </li>
            <li>
                <minSeverity>3.0</minSeverity>
                <label>extreme</label>
                <capMods>
                    <li>
                        <capacity>Sight</capacity>
                        <setMax>0.25</setMax>
                    </li>
                    <li>
                        <capacity>Hearing</capacity>
                        <setMax>0.25</setMax>
                    </li>
                </capMods>
            </li>
        </stages>
    </HediffDef>
